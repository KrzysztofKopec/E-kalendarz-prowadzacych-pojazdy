<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.krzy007.e_kalendarzprowadzacychpojazdy">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ppojazdy_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ppojazdy_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddWorkActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".ReviewOfWorksActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".EditionWorkActivity"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>